        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.571" />

    <Spinner
        android:id="@+id/spnTipoEmpleado"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:background="#FFFFFF"
